{
  "expo": {
    "name": "BlueBook",
    "slug": "BlueBook",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/WhiteBackground.png",
    "splash": {
      "image": "./assets/Loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#004990"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bluebooklocal.bluebook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/WhiteBackground.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/WhiteBackground.png"
    }
  }
}
